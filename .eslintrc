{
  "root":true,

  "env": {
    "node":true
  },

  "extends": ["eslint:recommended"],

  "rules": {
    "dot-notation":2,
    "eqeqeq": 2,
    "no-else-return":2,
    "no-fallthrough":2,
    "no-loop-func":2,
    "no-mixed-spaces-and-tabs":0,
    "no-return-assign":2,
    "no-self-compare":2,
    "no-shadow":2,
    "no-undef-init":2,
    "no-unused-expressions":2,
    "no-unused-vars": 2,
    "semi":[2,"always"],
    "strict": [2, "function"],

    "complexity":[1,4],
    "max-depth":[1,3]
  }
}
